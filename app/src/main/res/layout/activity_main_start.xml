<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="377dp"
        android:layout_height="118dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/singapore"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="371dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="upload"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="803dp"
        tools:text="@string/upload" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="discard"
        app:layout_constraintBaseline_toBaselineOf="@+id/button3"
        tools:layout_editor_absoluteX="173dp"
        tools:text="@string/discard" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cameraLabel"
        app:layout_constraintBaseline_toBaselineOf="@+id/editTextText"
        tools:layout_editor_absoluteX="70dp"
        tools:text="@string/camera" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="cameraType"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="520dp"
        tools:text="@string/camera_value" />
</android.support.constraint.ConstraintLayout>
